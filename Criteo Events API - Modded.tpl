// Imports
const sendHttpRequest = require('sendHttpRequest');
const getEventData = require('getEventData');
const getTimestampMillis = require('getTimestampMillis');
const JSON = require('JSON');
const getCookieValues = require('getCookieValues');
const logToConsole = require('logToConsole');
const parseUrl = require('parseUrl');
const Math = require('Math');


//--------- EDIT w.nieuwerth@adwise.nl

function getCriteoEventName(gtmEventName) {
  
  let e;
  let timestamp_millis = getTimestampMillis();
  let timestamp_unix = Math.round(timestamp_millis / 1000);
  
  switch (gtmEventName) {
      
    case 'page_view':
      if (parseUrl(getEventData('page_location')).pathname === '/') {
        e = {
          'event': 'viewHome',
          'timestamp': timestamp_unix
        };
      } else {
        e = {
          'event': 'viewPage',
          'timestamp': timestamp_unix
        };
      }
      break;
    case 'view_item_list': 
      e = {
        'event': 'viewList',
        'timestamp': timestamp_unix,
        'item': getEventData('items').map(item => item.item_id)
      };
      break;
    case 'view_item': 
      e = {
        'event': 'viewItem',
        'timestamp': timestamp_unix,
        'item': getEventData('items').map(item => item.item_id)
      };
      break;
    case 'add_to_cart': 
      e = {
        'event': 'addToCart',
        'timestamp': timestamp_unix,
        'currency': data.currency || undefined,
        'item': getEventData('items').map(item => ({
          'id': item.item_id,
          'price': item.price,
          'quantity': item.quantity
        }))
      };
      break;
    case 'view_cart': 
      e = {
        'event': 'viewBasket',
        'timestamp': timestamp_unix,
        'currency': data.currency || undefined,
        'item': getEventData('items').map(item => ({
          'id': item.item_id,
          'price': item.price,
          'quantity': item.quantity
        }))
      };
      break;
    case 'begin_checkout': 
      e = {
        'event': 'beginCheckout',
        'timestamp': timestamp_unix,
        'currency': data.currency || undefined,
        'item': getEventData('items').map(item => ({
          'id': item.item_id,
          'price': item.price,
          'quantity': item.quantity
        }))
      };
      break;
    case 'add_payment_info': 
      e = {
        'event': 'addPaymentInfo',
        'timestamp': timestamp_unix,
        'currency': data.currency || undefined,
        'item': getEventData('items').map(item => ({
          'id': item.item_id,
          'price': item.price,
          'quantity': item.quantity
        }))
      };
      break;
    case 'purchase': 
      e = {
        'event': 'trackTransaction',
        'timestamp': timestamp_unix,
        'id': getEventData('transaction_id'),
        'currency': data.currency || undefined,
        'item': getEventData('items').map(item => ({
          'id': item.item_id,
          'price': item.price,
          'quantity': item.quantity
        }))
      };
      break;
    case 'login': 
      e = {
        'event': 'login',
        'timestamp': timestamp_unix
      };
      break;
    default: e = {'event': gtmEventName};
      
  }
  
  return e;
  
}

var events = [];
events.push(getCriteoEventName(getEventData('event_name')));

//---------



// Constants (do not forget to update the version)
const tagVersion = 'criteo_sgtm_customized_by_adwise_0.0.3';
const COOKIE_NAME = "crto_mapped_user_id";
const OPTOUT_COOKIE = "crto_is_user_optout";

// Implementation
const postHeaders = {'Content-Type': 'application/json'};
const urlToCall = 'https://widget.criteo.com/m/event?version=s2s_v0';
var criteoMappedUserId = getCookieValues(COOKIE_NAME)[0];
var isUserOptOut = getCookieValues(OPTOUT_COOKIE)[0];

const postBodyData = {};
postBodyData.version = tagVersion;
postBodyData.site_type = "d"; // edited
postBodyData.account = data.partnerId; // edited
postBodyData.id = {'mapped_user_id': criteoMappedUserId,
                   'mapping_key': data.callerId
                  };
postBodyData.events = events; // edited
postBodyData.ip = getEventData('ip_override'); // edited



if (isUserOptOut === "true") {
  postBodyData.email = null;
  postBodyData.mapping_key = null;
  postBodyData.mapped_user_id = null;
  postBodyData.user_id = null;
  postBodyData.client_id = null;
  postBodyData.ip_override = null;
  postBodyData.ip = null;
}

const postBody = JSON.stringify(postBodyData);

// Fire
callWidget(urlToCall, 0);

function callWidget(widgetUrl, redirectLvl) {
    if (redirectLvl > 5) {
      logToConsole("Invalid Redirect Loop");
      data.gtmOnFailure();
      return;
    }
    sendHttpRequest(widgetUrl, (statusCode, headers, body) => {
    if (statusCode >= 200 && statusCode < 300) {
      data.gtmOnSuccess();
    } else if (statusCode >= 300 && statusCode < 400){
      callWidget(headers.location, redirectLvl + 1);
    } else {
      data.gtmOnFailure();
    }
  }, {headers: postHeaders, method: 'POST', timeout: 3000}, postBody);
}


